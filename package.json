{
  "name": "quickmock",
  "version": "1.0.0",
  "description": "Zero-config mock API server for frontend development",
  "type": "module",
  "bin": {
    "quickmock": "./bin/quickmock.js"
  },
  "main": "./dist/server.js",
  "types": "./dist/server.d.ts",
  "scripts": {
    "build": "tsc && cp -r src/dashboard dist/dashboard",
    "start": "node bin/quickmock.js",
    "dev": "tsx src/cli.ts"
  },
  "keywords": [
    "mock",
    "api",
    "server",
    "frontend",
    "development",
    "fake-data"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
